<script> 
var arr = [1, 8, 3, 2, 76, 3, 432] 
function test(arr){ 
 for (let i = 2; i < arr.length; i += 3){ 
 arr[i] = arr[i] * arr[i]; 
 } 
 arr.sort(function(a, b){ 
 if (a > b) return 1; 
 else return -1; 
 }) 
 return arr; 
} 
 
alert(arr); 
alert(test(arr)); 
 
 
</script> 
